{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "grouped-meta.schema.json",
	"type": "object",
	"required": [
		"biomes",
		"bugs",
		"aesthetics",
		"jace",
		"team",
		"in-game things",
		"creatures",
		"things players build"
	],
	"properties": {
		"biomes": {
			"type": "object",
			"required": [
				"ambiguity",
				"zones",
				"subzones"
			],
			"properties": {
				"ambiguity": {
					"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
				},
				"zones": {
					"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
				},
				"subzones": {
					"type": "object",
					"additionalProperties": {
						"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
					}
				}
			}
		},
		"bugs": {
			"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
		},
		"aesthetics": {
			"type": "object",
			"required": [
				"themes",
				"time of day",
				"other"
			],
			"properties": {
				"themes": {
					"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
				},
				"time of day": {
					"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
				},
				"other": {
					"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
				}
			}
		},
		"jace": {
			"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
		},
		"snutt": {
			"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
		},
		"mikael": {
			"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
		},
		"team": {
			"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
		},
		"in-game things": {
			"type": "object",
			"required": [
				"buildings",
				"vehicles",
				"other"
			],
			"properties": {
				"buildings": {
					"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
				},
				"vehicles": {
					"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
				},
				"other": {
					"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
				}
			}
		},
		"creatures": {
			"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
		},
		"things players build": {
			"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
		}
	},
	"additionalProperties": {
		"$ref": "../data/highlights-meta.json#/definitions/highlightsMeta"
	}
}

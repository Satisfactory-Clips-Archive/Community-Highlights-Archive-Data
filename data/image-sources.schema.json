{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"type": "object",
	"default": {},
	"title": "Image Source Schema",
	"additionalProperties": {
		"type": "array",
		"items": {
			"type": "object",
			"required": [
				"src",
				"alt",
				"skip"
			],
			"properties": {
				"additionalProperties": false,
				"src": {
					"type": "string"
				},
				"alt": {
					"type": "string"
				},
				"skip": {
					"type": "boolean"
				}
			}
		}
	}
}
